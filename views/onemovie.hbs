
<div id="one-movie" >
<img src="{{oneMovie.image}}" alt="">
<div>
    <a href="/movies">Go back</a>
    <h1>{{oneMovie.title}}</h1>
    <h2>Director: {{oneMovie.director}}</h2>
    <h2>Stars</h2>
    <ul>
        {{#each oneMovie.stars }}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p>{{oneMovie.description}}</p>
    <p>
    {{#each oneMovie.showtimes}}
    <span>{{this}} |</span>
    {{/each}}
    </p>
</div>
</div>
